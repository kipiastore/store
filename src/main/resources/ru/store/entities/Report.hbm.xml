<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.store.entities.Report" table="reports" >
        <id name="id" type="java.lang.Integer" >
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="name" type="string" >
            <column name="name" length="255" not-null="true" />
        </property>
        <property name="companyId" type="java.lang.Integer" >
            <column name="company_id" not-null="true" />
        </property>
        <property name="description" type="string" >
            <column name="description" length="255" not-null="false" />
        </property>
        <property name="lastModifiedDate" type="timestamp">
            <column name="last_modified_date" not-null="true" />
        </property>
        <property name="owner" type="string" >
            <column name="owner" length="45" not-null="true" />
        </property>
        <property name="createdDate" type="timestamp" >
            <column name="created_datetime" not-null="true" />
        </property>
        <property name="fileId" type="java.lang.Integer">
            <column name="file_id" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
