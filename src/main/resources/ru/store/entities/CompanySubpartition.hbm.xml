<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.store.entities.CompanySubPartition" table="company_subpartition" >
        <id name="id" type="java.lang.Integer" >
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="companyId" type="java.lang.Integer" >
            <column name="company_id" not-null="true" />
        </property>
        <property name="subPartitionId" type="java.lang.Integer" >
            <column name="subpartition_id" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
